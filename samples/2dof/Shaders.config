<?xml version="1.0" encoding="utf-8"?>
<ShaderWindowSettings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Width>640</Width>
  <Height>480</Height>
  <VSync>On</VSync>
  <WindowState>Maximized</WindowState>
  <DisplayDevice>Primary</DisplayDevice>
  <TargetRenderFrequency>0</TargetRenderFrequency>
  <RenderState>
    <StateConfiguration xsi:type="EnableState">
      <Capability>DepthTest</Capability>
    </StateConfiguration>
    <StateConfiguration xsi:type="DepthFunctionState">
      <Function>Less</Function>
    </StateConfiguration>
  </RenderState>
  <GraphicsMode>
    <ColorFormat>
      <Red>8</Red>
      <Green>8</Green>
      <Blue>8</Blue>
      <Alpha>8</Alpha>
    </ColorFormat>
    <Depth>16</Depth>
    <Stencil>0</Stencil>
    <Samples>4</Samples>
    <AccumulatorFormat>
      <Red>0</Red>
      <Green>0</Green>
      <Blue>0</Blue>
      <Alpha>0</Alpha>
    </AccumulatorFormat>
    <Buffers>2</Buffers>
    <Stereo>false</Stereo>
  </GraphicsMode>
  <Shaders>
    <ShaderConfiguration>
      <Name>Plane</Name>
      <Enabled>true</Enabled>
      <VertexShader>plane.vert</VertexShader>
      <FragmentShader>plane.frag</FragmentShader>
      <MeshName>Plane</MeshName>
      <RenderState />
      <TextureBindings />
      <FramebufferAttachments />
    </ShaderConfiguration>
    <ShaderConfiguration>
      <Name>Render</Name>
      <Enabled>true</Enabled>
      <VertexShader>point.vert</VertexShader>
      <FragmentShader>point.frag</FragmentShader>
      <RenderState />
      <TextureBindings />
      <FramebufferAttachments />
    </ShaderConfiguration>
  </Shaders>
  <Textures />
  <Meshes>
    <MeshConfiguration xsi:type="TexturedModel">
      <Name>Cube</Name>
      <FileName>cube.obj</FileName>
    </MeshConfiguration>
    <MeshConfiguration xsi:type="TexturedModel">
      <Name>Plane</Name>
      <FileName>plane.obj</FileName>
    </MeshConfiguration>
  </Meshes>
</ShaderWindowSettings>